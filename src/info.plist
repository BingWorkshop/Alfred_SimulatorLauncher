    print("  &lt;item uid=\"octal\" valid=\"yes\" arg=\"@"+item+"\"&gt;")
    print("    &lt;title&gt;"+item+"&lt;/title&gt;")
    print('    &lt;subtitle&gt;UDID: '+ item +'&lt;/subtitle&gt;')
    print('    &lt;icon&gt;android.png&lt;/icon&gt;')
    print('  &lt;/item&gt;')
  print('&lt;/items&gt;')


callback(simulator.ios_device_list(), simulator.android_device_list("/Users/Yourtion/Codes/Apps/AndroidSDK/"))
