    print("  &lt;item uid=\"octal\" valid=\"yes\" arg=\""+item[1]+"\"&gt;")
    print("    &lt;title&gt;"+item[0]+"&lt;/title&gt;")
    print('    &lt;subtitle&gt;UDID: '+ item[1] +'&lt;/subtitle&gt;')
    print('    &lt;icon&gt;ios.png&lt;/icon&gt;')
    print('  &lt;/item&gt;')
